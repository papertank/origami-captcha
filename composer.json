{
    "name": "origami/captcha",
    "description": "Simple Google Recaptcha integration for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank Limited",
            "homepage": "https://papertank.com",
            "email": "hello@papertank.com"
        },
        {
            "name": "David Rushton",
            "email": "david@papertank.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0",
        "google/recaptcha": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Captcha\\": "src/"
        }
