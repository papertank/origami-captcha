{
    "name": "origami/captcha",
    "description": "Simple Google Captcha integration for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "hello@papertank.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/validation": "^6.0|^7.0|^8.0|^9.0",
        "google/recaptcha": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Captcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Captcha\\CaptchaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
